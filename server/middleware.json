{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "helmet#xssFilter": {},
    "helmet#frameguard": {
      "params": {
        "action": "deny"
      }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 5184000,
        "includeSubdomains": true
      }
    },
    "helmet#hidePoweredBy": {
      "params": {
        "setTo": "PHP 7.2.6"
      }
    },
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "max-age": 864000
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {
      "params": { "limit": "5mb" }
    },
    "body-parser#urlencoded": {
      "params": {
        "limit": "5mb",
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": ["${restApiRoot}"]
    }
  },
  "files": {
    "loopback#static": [
      {
        "params": "$!../client"
      }
    ]
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
